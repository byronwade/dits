{
  "name": "@byronwade/dits",
  "version": "0.1.4",
  "description": "Version control for video and large binary files - like Git, but optimized for large binary files",
  "license": "Apache-2.0 OR MIT",
  "author": "Byron Wade",
    "repository": {
    "type": "git",
    "url": "git+https://github.com/byronwade/dits.git",
    "directory": "packages/npm"
  },
  "homepage": "https://github.com/byronwade/dits",
  "bugs": {
    "url": "https://github.com/byronwade/dits/issues"
  },
  "keywords": [
    "version-control",
    "video",
    "binary",
    "large-files",
    "vcs",
    "cli",
    "git",
    "media",
    "deduplication",
    "content-addressed",
    "chunking",
    "blake3"
  ],
  "bin": {
    "dits": "bin/dits"
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "echo 'Binaries built via cargo'",
    "clean": "rm -rf bin/darwin-* bin/linux-* bin/win32-*",
    "prepublishOnly": "node -e \"require('fs').accessSync('bin/darwin-arm64/dits')\" || (echo 'Error: Binary not found. Run: cargo build --release && cp target/release/dits packages/npm/bin/darwin-arm64/' && exit 1)"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
