{
  "name": "dits-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "build:cli": "cargo build --release",
    "test:cli": "cargo test",
    "bench": "node scripts/bench/run.mjs",
    "release": "turbo build && echo 'Ready for release'"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@next/swc-darwin-arm64": "^16.0.8",
    "next": "^16.0.8"
  }
}
